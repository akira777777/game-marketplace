{
  "version": 2,
  "rewrites": [
    {
      "source": "/api/:path*",
      "destination": "/backend/app/main.py"
    },
    {
      "source": "/(.*)",
      "destination": "/frontend/$1"
    }
  ],
  "redirects": [
    {
      "source": "/api",
      "destination": "/api/",
      "permanent": true
    }
  ]
}
